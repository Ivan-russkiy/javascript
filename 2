let str1 = 'Hello';
let str2 = 'Eric';

let num1 = 15;
let num2 = 2;

console.log(`${num1} + ${num2} = ${num1 + num2}`);
console.log(`${str1} ${str2} You are the best!`);
-----------------------------------------------------

num1 = num1 + 2;
num2 = num2 - 2;

console.log(`${num1} / ${num2} = ${num1 / num2}`);
